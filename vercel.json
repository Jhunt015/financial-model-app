{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/analyze-document.js": {
      "maxDuration": 300
    },
    "api/analyze-simple.js": {
      "maxDuration": 300
    },
    "api/analyze-vision.js": {
      "maxDuration": 300
    },
    "api/analyze-text-only.js": {
      "maxDuration": 300
    },
    "api/analyze-hybrid.js": {
      "maxDuration": 300
    },
    "api/analyze-hybrid-simple.js": {
      "maxDuration": 300
    },
    "api/analyze-intelligence.js": {
      "maxDuration": 300
    },
    "api/test.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}